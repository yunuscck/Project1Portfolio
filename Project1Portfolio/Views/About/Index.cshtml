@model List<Project1Portfolio.Models.About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Admin/Index.cshtml";
}

<table class=" table table-bordered">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Detail</th>
        @*<th>Foto</th>*@
        
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>@item.Title</td>
            <td>@item.Detail</td>
            @*<td><img src="@item.ImageUrl" alt="Yunus Cicek" width="75" height="100"></td>*@
            @*   Veri Ekleme Silme İşlemleri icin
                <td><a href="/Profile/DeleteProfile/@item.ProfileId" class="btn btn-danger">Sil</a></td>
            <td><a href="/Profile/UpdateProfile/@item.ProfileId" class="btn btn-success">Güncelle</a></td>*@
        </tr>
    }

</table>